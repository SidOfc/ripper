$spacing 10px
$blue    #09c
$gray    #ccc
$green   #6fa

.selector
  color #09c

  .first,
    fill #d22

  .second,
  .third
    border 1px solid black

    .child
      color #28f

  .yeah &
    background $blue

  .yeah&
    background $green

  .yeah & .nope
    user-select none

  .yeah &.nope
    user-select none
